<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <TextView
        android:text="Systrace"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:onClick="sysTraceActivity"
        style="@style/style_tv_normal"/>
    <TextView
        android:text="TraceView"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:onClick="traceViewActivity"
        style="@style/style_tv_normal"/>
    <TextView
        android:text="HierarchyView"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:onClick="hierarchyViewerActivity"
        style="@style/style_tv_normal"/>
    <TextView
        android:text="overDrawActivity"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:onClick="overDrawActivity"
        style="@style/style_tv_normal"/>
    <TextView
        android:text="trackerActivity"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:onClick="trackerActivity"/>

    <TextView
        android:text="MemoryActivity"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:onClick="memoryActivity"
        style="@style/style_tv_normal"/>
</LinearLayout>